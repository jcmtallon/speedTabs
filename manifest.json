{
    "name": "SpeedTabs",
    "short_name": "SpeedTabs",
    "version": "1.0.0.2",
    "options_page": "options.html",
    "description": "Search though open tabs, your bookmarks and the web faster!",
    "permissions": ["activeTab", "tabs", "bookmarks", "chrome://favicon/"],
    "icons": {
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    },
    "commands": {
      "_execute_browser_action": {
        "description": "Open search popup.",
        "suggested_key": {
          "default": "Ctrl+Q",
          "mac": "MacCtrl+Q"
        }
      }
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "SpeedTabs",
      "default_icon": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      }
    },
    "manifest_version": 2
  }