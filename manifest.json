{
    "name": "Chrome Quick!",
    "version": "1.0",
    "options_page": "options.html",
    "description": "Search though open tabs, your bookmarks and the web faster!",
    "permissions": ["activeTab", "tabs", "bookmarks", "chrome://favicon/"],
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "commands": {
      "_execute_browser_action": {
        "description": "Open search popup.",
        "suggested_key": {
          "default": "Ctrl+Q",
          "mac": "MacCtrl+Q"
        }
      }
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "CromeQuick",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "manifest_version": 2
  }






